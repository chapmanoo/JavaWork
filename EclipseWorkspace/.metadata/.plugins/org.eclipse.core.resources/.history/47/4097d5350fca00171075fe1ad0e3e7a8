
public class FactorialFinder {

	public static void main(String[] args)
	{
		int counter = 1;
		int target = 740;
		int temp = target;
		
		String s = "";
		
		while (target != counter)
		{
			if (target%counter == 0) {
				target = target/counter;
				counter++;
			}
			else {
				s = "NONE";
				break;
			}
			
		}
		if (target == 0)
		{
		System.out.println(temp + " = " + counter + "!");
		}
		else
		{
			System.out.println(s);
		}
	}
}
