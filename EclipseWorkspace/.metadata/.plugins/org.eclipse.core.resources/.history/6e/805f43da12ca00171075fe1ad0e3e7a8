
public class FactorialFinder {

	public static void main(String[] args) {
		int counter = 1;
		int target = 18;
		int temp = target;
		boolean NONE = false;

		String s = "";

		while (target != counter) {
			if (target % counter == 0) {
				target = target / counter;
				counter++;
			} else {
				NONE = true;
				break;
			}
		}
		if (!NONE) {
			System.out.println(temp + " = " + counter + "!");
		} else {
			System.out.println(temp + " NONE");
		}

	}
	
	for(int i: intArray) {
		ahsdhasdi
	}
	
	foreach(int i in intArray){
		ajsdhasasd
	}
	
	
	
	static int method1() {
		
		System.out.println(" NONE");
		return 0;
		
	} 
}
